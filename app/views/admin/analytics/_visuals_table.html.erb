<table>

  <thead>
    <tr>

      <th>
        Visualization Name
      </th>

      <th>
        Views
      </th>

      <th>
        Facebook Shares
      </th>

      <th>
        Engagement Rating
      </th>

    </tr>
  </thead>

  <tbody>

    <% @visualizations.each do |visualization| %>

      <tr>

        <th>
          <%= visualization.title %>
        </th>

        <th>
          <%= visualization.impressions_count %>
        </th>

        <th>
          <%= visualization.fb_likes %>
        </th>

        <th>
          <%= visualization.engagement_rating %>
        </th>

      </tr>

    <% end %>

  </tbody>

</table>
