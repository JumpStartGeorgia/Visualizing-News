<%- model_class = VisualizationType -%>
<%- model_class_trans = VisualizationTypeTranslation -%>
<%= page_title t '.title', :default => model_class.model_name.human %>

<p><strong><%= model_class_trans.human_attribute_name(:name) %>:</strong>
<%= @visualization_type.name %></p>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              admin_visualization_types_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_visualization_type_path(@visualization_type), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_visualization_type_path(@visualization_type),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
</div>
