<table class='table js-dynatable'>

  <thead>
    <tr>

      <th>
        <%= t '.columns.title' %>
      </th>

      <th>
        <%= t '.columns.views' %>
      </th>

      <th>
        <%= t '.columns.facebook_shares' %>
      </th>

      <th>
        <%= t '.columns.engagement_rating' %>
      </th>

    </tr>
  </thead>

  <tbody>

    <% @visualizations.each do |visualization| %>

      <tr>

        <th>
          <%= visualization.title %>
        </th>

        <th>
          <%= visualization.impressions_count %>
        </th>

        <th>
          <%= visualization.fb_likes %>
        </th>

        <th>
          <%= visualization.engagement_rating %>
        </th>

      </tr>

    <% end %>

  </tbody>

</table>
